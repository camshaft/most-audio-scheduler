{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-config-standard"
  ],
  "rules": {
    "import-name": false,
    "missing-jsdoc": false,
    "completed-docs": false,
    "no-relative-imports": false,
    "no-import-side-effect": false,
    "export-name": false,
    "no-suspicious-comment": false,
    "no-reference": false,
    "no-this-assignment": false
  }
}
